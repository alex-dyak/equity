{
    "name": "w4p/package",
    "type": "project",
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.5",
        "composer/installers": "~1.0.12",
        "vlucas/phpdotenv": "^2.0.1",
        "johnpbloch/wordpress": ">4.5",
        "oscarotero/env": "^1.0",
        "roots/wp-password-bcrypt": "1.0.0",
        "wpackagist-plugin/wordpress-seo": "*",
        "wpackagist-plugin/wysiwyg-widgets": ">2.3.0",
        "wpackagist-plugin/tinymce-advanced": ">4.3.10",
        "wpackagist-plugin/regenerate-thumbnails": ">2.2.5",
        "wpackagist-plugin/svg-support": ">2.2.3",
        "wpackagist-plugin/types": ">2.0",
        "wpackagist-plugin/contact-form-7": ">4.4.2",
        "wpackagist-plugin/post-types-order": ">=1.9",
        "wpackagist-plugin/cloudflare": ">3.0.5",
        "wpackagist-plugin/cloudflare-flexible-ssl": ">1.2.0",
        "wpackagist-plugin/pc-google-analytics": ">=1.1",
        "wpackagist-plugin/better-search-replace": ">=1.2.10",
        "wpackagist-plugin/faq-wd": ">=1.0.20",
        "wpackagist-plugin/capability-manager-enhanced": ">1.5.5",
        "wpackagist-plugin/duplicate-page": ">=2.1",
        "wpackagist-plugin/google-sitemap-generator": ">=4.0.7.1",
        "wpackagist-plugin/leadin": ">=6.1.0",
        "wpackagist-plugin/hubspot-tracking-code": ">=1.1.0",
        "wpackagist-plugin/lazy-load": ">=0.6",
        "wpackagist-plugin/share-this": ">=7.8",
        "wpackagist-plugin/updraftplus": ">=1.12.29",
        "wpackagist-plugin/user-role-editor": ">=4.28",
        "wpackagist-plugin/w3-total-cache": ">=0.9.5.1",
        "wpackagist-plugin/wp-google-analytics-events": ">=2.2",
        "wpackagist-plugin/yandex-metrica": ">=1.4.2",
        "wpackagist-plugin/zopim-live-chat": ">=1.4.4",
        "wpackagist-plugin/wp-testimonial-with-widget": ">=2.2.4",
        "wpackagist-plugin/wp-pagenavi": ">2.90",
        "wpackagist-plugin/ewww-image-optimizer": "3.1.3"
    },
    "extra": {
        "installer-paths": {
            "wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "wp"
    }
}
