{
    "name": "w4p/package",
    "type": "project",
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.5",
        "composer/installers": "~1.0.12",
        "vlucas/phpdotenv": "^2.0.1",
        "johnpbloch/wordpress": ">4.5",
        "oscarotero/env": "^1.0",
        "roots/wp-password-bcrypt": "1.0.0",
        "wpackagist-plugin/wordpress-seo": "*",
        "wpackagist-plugin/wysiwyg-widgets": ">2.3.0",
        "wpackagist-plugin/tinymce-advanced": ">4.3.10",
        "wpackagist-plugin/regenerate-thumbnails": ">2.2.5",
        "wpackagist-plugin/svg-support": ">2.2.3",
        "wpackagist-plugin/types": ">2.0",
        "wpackagist-plugin/contact-form-7": ">4.4.2",
        "wpackagist-plugin/post-types-order": ">=1.9",
        "wpackagist-plugin/cloudflare": ">3.0.5",
        "wpackagist-plugin/cloudflare-flexible-ssl": ">1.2.0",
        "wpackagist-plugin/pc-google-analytics": ">=1.1",
        "wpackagist-plugin/better-search-replace": ">=1.2.10",
        "wpackagist-plugin/faq-wd": ">=1.0.20"
    },
    "extra": {
        "installer-paths": {
            "wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "wp"
    }
}
